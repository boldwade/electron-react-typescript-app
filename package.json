{
  "name": "electron-react-typescript-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "react-scripts build",
    "build:electron": "electron-builder",
    "eject": "react-scripts eject",
    "electron": "electron .",
    "package": "yarn build && electron-builder build --publish never",
    "start-2": "concurrently \"cross-env BROWSER=none yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "start-3": "concurrently \"yarn start:nobrowser\" \"wait-on http://localhost:3000 && yarn electron\"",
    "start": "react-scripts start",
    "start:nobrowser": "cross-env BROWSER=none react-scripts start",
    "start:electron": "npm-run-all -p start:nobrowser electron",
    "test": "react-scripts test"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "typescript": "~3.7.2"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "electron": "^10.1.2",
    "electron-builder": "^22.8.1",
    "electron-is-dev": "^1.2.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5"
  },
  "homepage": "./",
  "main": "public/electron.js",
  "build": {
    "productName": "WVC",
    "appId": "org.instantbits.wvc",
    "directories": {
      "buildResources": "build",
      "output": "release"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "icon": "build/logo512.png"
    }
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "author": {
    "name": "InstantBits WVC",
    "email": "wvc@instantbits.com",
    "url": "https://instantbits.com"
  },
  "contributors": [
    {
      "name": "Carlos",
      "email": "carlos@gmail.com"
    },
    {
      "name": "Jeff Tyson",
      "email": "jeff.tyson@gmail.com"
    },
    {
      "name": "Wade Walker",
      "email": "wade.s.walker@gmail.com"
    }
  ],
  "license": "MIT",
  "description": "Cross platform WVC app"
}
